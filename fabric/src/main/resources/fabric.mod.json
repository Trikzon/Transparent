    "authors": [
        "Trikzon"
    ],
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/transparent-fabric",
        "issues": "https://github.com/trikzon/transparent/issues",
        "sources": "https://github.com/trikzon/transparent"
    },
    "license": "MIT",
    "icon": "assets/transparent/icon.png",
